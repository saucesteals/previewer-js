  "author": "Daniel <saucesteals@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@types/tough-cookie": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
